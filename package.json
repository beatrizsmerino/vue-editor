{
	"version": "1.0.9",
	"private": true,
	"name": "vue-editor",
	"description": "Editor online of code made with Vue.",
	"author": "beatrizsmerino@gmail.com",
	"license": "ISC",
	"main": "index.js",
	"type": "module",
	"scripts": {
		"install:clean": "rm -rf node_modules package-lock.json",
		"dependencies:fix": "npm install --ignore-scripts && npx npm-force-resolutions",
		"preinstall": "npm run install:clean && npm run dependencies:fix",
		"postinstall": "npm run lint",
		"prepare": "husky install",
		"dev": "vite",
		"build": "vite build",
		"preview": "vite preview",
		"prettier:fix": "prettier --write \"./**/*.{vue,js,mjs,json}\"",
		"eslint:fix": "eslint \"**/*.{json,js,mjs,vue}\" --fix --ignore-path .eslintignore",
		"stylelint:fix": "stylelint \"**/*.{css,scss,sass,vue}\" --fix --ignore-path .stylelintignore",
		"lint": "npm run prettier:fix && npm run eslint:fix && npm run stylelint:fix",
		"deploy": "node gh-pages-deploy.mjs"
	},
	"dependencies": {
		"chalk": "^5.3.0",
		"execa": "^7.2.0",
		"json-editor-vue": "^0.12.0",
		"node-emoji": "^2.1.3",
		"v-clipboard": "^3.0.0-next.1",
		"vite-plugin-svg-icons": "^2.0.1",
		"vue": "^2.7.16",
		"vue-router": "^3.6.5",
		"vuex": "^3.6.2"
	},
	"devDependencies": {
		"@commitlint/cli": "^18.6.1",
		"@commitlint/config-conventional": "^18.6.3",
		"@vitejs/plugin-vue2": "^2.3.1",
		"@vue/eslint-config-prettier": "^9.0.0",
		"eslint": "^8.57.0",
		"eslint-plugin-json": "^3.1.0",
		"eslint-plugin-prettier": "^5.1.3",
		"eslint-plugin-vue": "^9.26.0",
		"husky": "^8.0.3",
		"lint-staged": "^15.2.2",
		"postcss": "^8.4.38",
		"postcss-html": "^1.7.0",
		"prettier": "^3.2.5",
		"sass": "^1.77.2",
		"stylelint": "^16.5.0",
		"stylelint-config-property-sort-order-smacss": "^10.0.0",
		"stylelint-config-recommended-vue": "^1.5.0",
		"stylelint-config-sass-guidelines": "^11.1.0",
		"stylelint-config-standard": "^36.0.0",
		"stylelint-scss": "^6.3.0",
		"vite": "^5.1.6"
	},
	"resolutions": {},
	"engines": {
		"node": "20.11.0",
		"npm": "10.4.0"
	}
}
