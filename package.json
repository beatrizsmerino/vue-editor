{
	"name": "vue-editor",
	"version": "0.1.0",
	"private": true,
	"scripts": {
		"preinstall": "npm install --package-lock-only --ignore-scripts && npx npm-force-resolutions",
		"serve": "vue-cli-service serve",
		"build": "vue-cli-service build",
		"prettier:fix": "prettier --write \"./**/*.{vue,js,json}\"",
		"eslint:fix": "vue-cli-service lint \"**/*.{json,js,vue}\" --fix --ignore-path .eslintignore",
		"lint": "npm run prettier:fix && npm run eslint:fix"
	},
	"dependencies": {
		"core-js": "^3.20.1",
		"vue": "^2.6.14",
		"vue-router": "^3.5.3",
		"vuex": "^3.6.2"
	},
	"devDependencies": {
		"@babel/core": "^7.16.5",
		"@babel/eslint-parser": "^7.16.5",
		"@vue/cli-plugin-babel": "~4.5.15",
		"@vue/cli-plugin-eslint": "~4.5.15",
		"@vue/cli-plugin-router": "~4.5.15",
		"@vue/cli-plugin-vuex": "~4.5.15",
		"@vue/cli-service": "~4.5.15",
		"@vue/eslint-config-prettier": "^6.0.0",
		"eslint": "^7.32.0",
		"eslint-plugin-json": "^3.1.0",
		"eslint-plugin-prettier": "^3.4.1",
		"eslint-plugin-vue": "^6.2.2",
		"eslint-webpack-plugin": "^3.1.1",
		"prettier": "^2.5.1",
		"sass": "^1.45.1",
		"sass-loader": "^10.0.0",
		"vue-template-compiler": "^2.6.14"
	},
	"resolutions": {
		"ansi-regex": "https://registry.npmjs.org/ansi-regex/-/ansi-regex-5.0.1.tgz",
		"glob-parent": "https://registry.npmjs.org/glob-parent/-/glob-parent-5.1.2.tgz",
		"nth-check": "https://registry.npmjs.org/nth-check/-/nth-check-2.0.1.tgz"
	}
}
